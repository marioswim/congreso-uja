{% extends "base.html.twig" %}

{% block title %}   {{title_page}}  {% endblock %}

 
{%block body %}
<div id="portada">
	<img src="{{asset('css/images/portada.jpg')}}">
</div>
<div class="container">
    
    <div id="patners">
		
		{% for rol, group in patners %}
			<h1>{{rol}}</h1>

			{% for id, patner in group %}
				

				<div id="{{id}}" class="col col-lg-3 vcenter">
					<span></span>
					<img src="{{asset(patner.uri)}}" title="{{patner.nombre}}" alt="{{patner.nombre}}"/>
				</div>

			{% endfor %}
		{% endfor %}
	</div>
</div>
    
    <div>
    	
    	<iframe src="{{map}}" width="100%" height="500px"></iframe>
    </div>

{% endblock %}


	



{% block footer %}


    <div class="container">
        

    </div>

{% endblock %}
